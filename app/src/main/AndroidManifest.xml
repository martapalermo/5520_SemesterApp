<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="edu.neu.madcourse.madsu21_martapalermo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher_mp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_mp_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MADSu21MartaPalermo"
        android:usesCleartextTraffic="true"
        tools:targetAPI="m">
        <activity android:name=".A6"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".A1"
            android:configChanges="orientation|screenSize|keyboardHidden" />
        <activity
            android:name=".A3"
            android:configChanges="orientation|screenSize|keyboardHidden" />
        <activity
            android:name=".A4"
            android:configChanges="orientation|screenSize|keyboardHidden" />
        <activity
            android:name=".A5"
            android:configChanges="orientation|screenSize|keyboardHidden">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>